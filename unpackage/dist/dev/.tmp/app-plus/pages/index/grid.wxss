view {
		font-size: 28rpx;
		line-height: inherit
}
.example {
		/* padding: 0 30upx 30upx */
}
.uni-notice-bar{
		margin-bottom: 0px;
}
.example-title {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		font-size: 32rpx;
		color: #464e52;
		padding: 30rpx 30rpx 30rpx 50rpx;
		/* margin-top: 20upx; */
		position: relative;
		background-color: #fdfdfd;
		border-bottom: 1px #f5f5f5 solid
}
.example-title__after {
		position: relative;
		color: #031e3c
}
.example-title:after {
		content: '';
		position: absolute;
		left: 30rpx;
		margin: auto;
		top: 0;
		bottom: 0;
		width: 6rpx;
		height: 32rpx;
		background-color: #ccc
}
.example .example-title {
		margin: 0rpx 0
}
.example-body {
		padding: 30rpx;
		background: #fff
}
.example-info {
		padding: 30rpx;
		color: #3b4144;
		background: #fff
}
.image {
		width: 50rpx;
		height: 50rpx;
}
.text {
		font-size: 26rpx;
		margin-top: 10rpx;
}

